{
  "name": "Link speed",
  "manifest_version": 2,
  "version": "0.1",
  "options_page": "options.html",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {

  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["annotater.js"]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "<all_urls>"
  ]
}
